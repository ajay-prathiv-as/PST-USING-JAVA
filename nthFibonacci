import java.io.*;
import  java.util.*;
class UserMAinCode
{
  public long nthFibonacci(int input1){
    if(input1 == 0) return 0;
    if(input1 == 1) return 1;
    int a = 0,b = 1;
    for(int i = 2;i <= input1;i++)
      {
          int c = a + b;
          a = b;
          b = c;
      }
      return b;
	}
}
